
@{
    ViewData["Title"] = "Painel Administrativo";
}

<h1>Usuários</h1>
<hr />


<div class="card-container">
    <a asp-controller="Admin" asp-action="ListarAlunos">
        <div class="card" style="width: 18rem;">
            <img src="data:image/png;base64," class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title">Alunos</h5>
          </div>
        </div>
    </a>

    <a asp-controller="Admin" asp-action="ListarProfessores">
        <div class="card" style="width: 18rem;">
            <img src="data:image/png;base64," class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">Professores</h5>
           
            </div>
        </div>
    </a>

    <a asp-controller="Admin" asp-action="ListarCoordenadores">
        <div class="card" style="width: 18rem;">
            <img src="data:image/png;base64," class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">Coordenadores</h5>
            
            </div>
        </div>
    </a>
</div>